extends layout

block content
 div(class="container")
  div(class="row")
   div(class='content-form')
    form
    label(for='datepicker') Fecha Inicio
    input#datepicker(type='text',class='form-group', name='datepicker', readonly='readonly', size='12')

    label(for='datepicker2') Fecha Fin
    input#datepicker2(type='text',class='form-group', name='datepicker2',  readonly='readonly', size='12')

    label(for='time') Hora
    input#time(type='time',class='form-group', name='time')

    select(class='form-group' id='type_graphic' name='type_graphic')
     option(value='TipodeGrafica') Tipo Grafica
     option(value='uno') tipo uno
     option(value='dos') tipo dos
     option(value='tres') tipo tres
     option(value='cuatro') tipo cuatro

    select(class='form-group' id='type_value' name='type_value')
     option(value='Tipo Valor') Tipo Valor
     option(value='uno') valor uno
     option(value='dos') valor dos
     option(value='tres') valor  tres
     option(value='cuatro') valor cuatro

    input(type='submit',class='btn btn-default', name='',id="send", value='Generar Reporte')

     

    .controlesSegundarios

    .contenidoReporte

  script(type='text/javascript' src='/javascripts/ajax/ajaxfunctions.js')
 
 
    